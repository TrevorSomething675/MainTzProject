services:
  db:
    environment:
      POSTGRES_DB: CarCompanyDb
      POSTGRES_PASSWORD: postgres
      POSTGRES_USER: postgres
    image: postgres
    networks:
      CarCompany: null
    ports:
      - mode: ingress
        target: 5432
        published: "5437"
        protocol: tcp
    restart: always
  maintz.restapi:
