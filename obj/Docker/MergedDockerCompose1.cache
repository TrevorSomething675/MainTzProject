name: dockercompose3432824561139095138
services:
  db:
    environment:
      POSTGRES_DB: CarsDataBase
      POSTGRES_PASSWORD: admin
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      CarCompany_Network: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: pgdata
        target: /var/lib/postgresql/data
        volume: {}
  maintz.restapi:
    build:
      context: C:\Users\Иван\Documents\GitHub\MainTzProject
      dockerfile: MainTz.RestApi\Dockerfile
      args:
        BUILD_CONFIGURATION: Debug
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MainTz.RestApi
      target: base
    container_name: MainTz.RestApi
    depends_on:
      db:
        condition: service_started
        required: true
    entrypoint:
      - C:\remote_debugger\x64\msvsmon.exe
      - /noauth
      - /anyuser
      - /silent
      - /nostatus
      - /noclrwarn
      - /nosecuritywarn
      - /nofirewallwarn
      - /nowowwarn
      - /timeout:2147483646
      - /LogDebuggeeOutputToStdOut
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DATABASE_URL: postgresql://postgres:admin@db:5432/CarsDataBase
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
      NUGET_PACKAGES: C:\.nuget\packages
    image: maintzrestapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath c:\.nuget\packages  "C:\app\bin\Debug\net8.0\MainTz.RestApi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: C:\remote_debugger\x64\utils\KillProcess.exe dotnet.exe
      com.microsoft.visualstudio.debuggee.program: '"C:\Program Files\dotnet\dotnet.exe"'
      com.microsoft.visualstudio.debuggee.workingdirectory: C:\app
    networks:
      CarCompany_Network: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Program Files\Microsoft Visual Studio\2022\Community\Common7\IDE\CommonExtensions\Microsoft\HotReload
        target: C:\HotReloadAgent
        read_only: true
        bind:
          create_host_path: true
