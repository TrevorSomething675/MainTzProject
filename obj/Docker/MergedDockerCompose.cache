name: dockercompose3432824561139095138
services:
  db:
    environment:
      POSTGRES_DB: CarsDataBase
      POSTGRES_PASSWORD: admin
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      CarCompany_Network: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: pgdata
        target: /var/lib/postgresql/data
        volume: {}
  maintz.restapi:
    build:
      context: C:\Users\Иван\Documents\GitHub\MainTzProject
      dockerfile: MainTz.RestApi\Dockerfile
    depends_on:
      db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      DATABASE_URL: postgresql://postgres:admin@db:5432/CarsDataBase
    image: maintzrestapi
    networks:
      CarCompany_Network: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Иван\AppData\Roaming/Microsoft/UserSecrets
        target: C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Иван\AppData\Roaming/ASP.NET/Https
        target: C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https
        read_only: true
        bind:
          create_host_path: true
networks:
  CarCompany_Network:
    name: dockercompose3432824561139095138_CarCompany_Network
volumes:
  pgdata:
    name: dockercompose3432824561139095138_pgdata